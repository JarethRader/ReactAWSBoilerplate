{
  "name": "frontend",
  "version": "1.0.0",
  "description": "React frontend for ReactJS-AWS project",
  "main": "src/index.ts",
  "scripts": {
    "start": "env NODE_EVN=development webpack serve --mode development --config webpack.dev.js",
    "build:dev": "env NODE_ENV=development webpack --mode development --config webpack.dev.js",
    "build:prod": "env NODE_ENV=production webpack --mode produciton --config webpack.prod.js",
    "css": "npx tailwindcss build -i tailwind/tailwind.css -o src/style.css"
  },
